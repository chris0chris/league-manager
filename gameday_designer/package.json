    "i18next-resources-for-ts": "^2.0.0",
    "jsdom": "^27.0.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.3",
    "vite": "^7.0.0",
    "vitest": "^4.0.0"
  }
}
