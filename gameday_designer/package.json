    "debug": "^4.4.0",
    "xml2js": "^0.6.2",
    "typescript": "^5.9.3"
  },
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run --coverage",
    "test:coverage": "vitest run --coverage",
    "eslint": "eslint ./src",
    "eslint:junit": "eslint ./src --format junit -o /tmp/eslint/js-lint-results-gameday_designer.xml",
    "eslint:json": "eslint ./src --format json -o js-lint-results-gameday_designer.json",
    "dev": "vite build",
    "i18n:types": "i18next-resources-for-ts toc -i ./src/i18n/locales/de -o ./src/i18n/types.d.ts",
    "prebuild": "npm run i18n:types"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-transform-private-property-in-object": "^7.27.1",
    "@eslint/js": "^9.39.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/uuid": "^11.0.0",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitest/coverage-v8": "^4.0.0",
    "@vitest/ui": "^4.0.0",
