    "i18next": "^25.7.3",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-resources-to-backend": "^1.2.1",
    "react": "^19.2.0",
    "react-bootstrap": "^2.10.10",
    "react-dom": "^19.2.0",
    "react-i18next": "^16.5.1",
    "react-select": "^5.10.2",
    "uuid": "^13.0.0",
    "web-vitals": "^5.1.0"
  },
  "overrides": {
